<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="680dip"
    android:layout_height="408dip"
    android:layout_gravity="center"
    android:background="@drawable/icon_bg1" >

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="@dimen/lang_top_margin"
        android:text="@string/keyboard_setting"
        android:textColor="@color/white"
        android:textSize="@dimen/lang_title_sp" />

    <ListView
        android:id="@+id/keyboard_lv"
        android:layout_width="@dimen/lang_list_width"
        android:layout_height="300dip"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="70dip"
        android:divider="@drawable/list_divider"
        android:listSelector="@drawable/select_item_bg"
        android:scrollbars="none" />

</FrameLayout>