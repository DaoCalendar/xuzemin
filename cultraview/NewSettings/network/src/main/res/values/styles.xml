<resources>

    <style name="AppBaseTheme" parent="android:Theme.Light"></style>

    <style name="wire_fl_sty">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:layout_gravity">center_horizontal</item>
    </style>

    <style name="wire_left_tv">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">left</item>
        <item name="android:textSize">@dimen/content_sp</item>
        <item name="android:layout_gravity">left|center_vertical</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:layout_marginLeft">@dimen/fab_network_margin</item>
    </style>

    <style name="wire_right_edt1">
        <item name="android:layout_width">@dimen/dp_40</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">@dimen/content_sp</item>
        <item name="android:layout_gravity">right|center_vertical</item>
        <item name="android:textColor">@drawable/item_text_color</item>
        <item name="android:layout_marginRight">@dimen/dp_150</item>
        <item name="android:background">@drawable/transparency_bg</item>
        <item name="android:imeOptions">actionNext|flagNoExtractUi|flagNoFullscreen</item>
        <item name="android:inputType">number</item>
        <item name="android:maxLength">3</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColorHighlight">@color/edit_select</item>
    </style>

